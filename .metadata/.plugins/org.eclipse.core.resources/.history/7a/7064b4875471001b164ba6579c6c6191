
#include "My_Keypad.h"
#include"My_Lcd.h"
#include"My_adc.h"

//uint8 key=0;


int main()
{
	uint16 reader=0;
	LCD_init();
	ADC_init();
	LCD_clearScreen();
	LCD_displayStringRowColumn(0,2,"ADC value=");


	while(1)
	{
		reader=ADC_readChannel(0);
		LCD_goToRowColumn(0,12);
		LCD_intgerToString(reader);


	}
	return 0;
}


